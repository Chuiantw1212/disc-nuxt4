{
  "functions": {
    "source": ".output/server",
    "runtime": "nodejs20",
    "predeploy": [
      "npm install"
    ]
  },
  "hosting": {
    "public": ".output/public",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "server",
        "pinTag": true
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}